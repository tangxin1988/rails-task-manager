<%= form_with model: model_name do |f| %>
  <p><%= f.label :title %></p>
  <p><%= f.text_field :title %></p>
  <p><%= f.label :details %></p>
  <p><%= f.text_field :details %></p>
  <p>
    <%= f.check_box :completed, checked_value: :true, unchecked_value: :false %>
    <%= f.label :completed %>
  </p>
  <p><%= f.submit %></p>
<% end %>
